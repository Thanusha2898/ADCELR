#include <stdio.h>

int main()
{
    int arr[] = {2,6,0,8,6,0,3,9};
    
    int i, count=0;
    int n=8;
    for(i=0;i<n;i++) 
    {
       if(arr[i]>0)
       {
         arr[count++]=arr[i];
       }
       
   }
   while(count<n)
   {
       arr[count++]=0;
   }
   for(i=0;i<n;i++) 
   {
      printf(" %d ",arr[i]);
   }
    
    return 0;
}
