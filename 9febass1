#include <stdio.h>
int main()
{
int a=5; 
int *p; 
p=&a; 
printf(" Variable adress :%u",p); 
p=p+1;
printf("\n Variable adress :%u",p);
p=p+2;
printf("\n Variable adress :%u",p);
p=p-1; 
printf("\n Variable adress :%u",p); 
p=p-4; 
printf("\n Variable adress :%u",p);
return 0;
}
